<?xml version="1.0"?>
<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/layout_generic.xsd">
    <container name="root">
        <block class="Magento\Framework\View\Element\Template" name="developertoolbar.styles" template="Sga_DeveloperToolbar::styles.phtml" />
        <block class="Sga\DeveloperToolbar\Block\Toolbar" name="developertoolbar.toolbar" template="Sga_DeveloperToolbar::toolbar.phtml">
            <block class="Sga\DeveloperToolbar\Block\Toolbar\Request" name="developertoolbar.toolbar.request" template="Sga_DeveloperToolbar::toolbar/request.phtml" />
            <block class="Sga\DeveloperToolbar\Block\Toolbar\Layout" name="developertoolbar.toolbar.layout" template="Sga_DeveloperToolbar::toolbar/layout.phtml" />
            <block class="Sga\DeveloperToolbar\Block\Toolbar\Events" name="developertoolbar.toolbar.events" template="Sga_DeveloperToolbar::toolbar/events.phtml" />
            <block class="Sga\DeveloperToolbar\Block\Toolbar\Models" name="developertoolbar.toolbar.models" template="Sga_DeveloperToolbar::toolbar/models.phtml" />
            <block class="Sga\DeveloperToolbar\Block\Toolbar\Plugins" name="developertoolbar.toolbar.plugins" template="Sga_DeveloperToolbar::toolbar/plugins.phtml" />
            <block class="Sga\DeveloperToolbar\Block\Toolbar\Database" name="developertoolbar.toolbar.database" template="Sga_DeveloperToolbar::toolbar/database.phtml" />
            <block class="Sga\DeveloperToolbar\Block\Toolbar\Profiler" name="developertoolbar.toolbar.profiler" template="Sga_DeveloperToolbar::toolbar/profiler.phtml" />
            <block class="Sga\DeveloperToolbar\Block\Toolbar\Memory" name="developertoolbar.toolbar.memory" />
            <block class="Sga\DeveloperToolbar\Block\Toolbar\Time" name="developertoolbar.toolbar.time" />
        </block>
    </container>
</layout>
